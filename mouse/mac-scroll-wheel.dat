<?xml version="1.0" encoding="utf-8"?>
<obprofiles xmlns="http://www.logitech.com/Cassandra/2013.1/OnboardProfile">
  <profile name="Mac (scroll)">
    <device model="Logitech.Gaming.Mouse.G700" reportrate="125" buttoncount="13" anglesnapping="0" powermode="1">
      <dpitable defaultindex="0" currentindex="0">
        <dpi x="1150" y="1150"/>
        <dpi x="0" y="0"/>
        <dpi x="0" y="0"/>
        <dpi x="0" y="0"/>
        <dpi x="0" y="0"/>
      </dpitable>
    </device>
    <buttons>
      <button number="1" name="Left Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="leftclick"/>
        </mousefunction>
      </button>
      <button number="2" name="Right Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="rightclick"/>
        </mousefunction>
      </button>
      <button number="3" name="Middle Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="middleclick"/>
        </mousefunction>
      </button>
      <button number="4" name="Left Cmd + [">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="LBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="5" name="Left Cmd + ]">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="RBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="6" name="Left Cmd + C">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="C"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="7" name="Left Cmd + V">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="V"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="8" name="Left Cmd + W">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="W"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="9" name="Return">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="ENTER"/>
        </keystroke>
      </button>
      <button number="10" name="Ctrl + Up">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="UP"/>
          <modifier value="LCTRL"/>
        </keystroke>
      </button>
      <button number="11" name="Switch Profile">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="switchprofile"/>
        </mousefunction>
      </button>
      <button number="12" name="Scroll Left">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="scrollleft"/>
        </mousefunction>
      </button>
      <button number="13" name="Scroll Right">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="scrollright"/>
        </mousefunction>
      </button>
    </buttons>
  </profile>
</obprofiles>
